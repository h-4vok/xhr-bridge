{
  "name": "xhr-bridge",
  "version": "1.0.0",
  "description": "XHR Bridge Pattern that allows you to decouple your codebase from the actual XHR implementation of your choice. You will need an XHR-Bridge implementation of the XHR library of your choosing.",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "prettier": "pretty-quick --staged",
    "prettier:full": "pretty-quick --branch master --verbose",
    "prettier:ci-check": "prettier --config ./.prettierrc.yml --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "lint": "eslint *.js --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-4vok/xhr-bridge.git"
  },
  "keywords": [
    "xhr",
    "api",
    "fetch",
    "superagent",
    "axios"
  ],
  "author": "Christian Guzm√°n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h-4vok/xhr-bridge/issues"
  },
  "homepage": "https://github.com/h-4vok/xhr-bridge#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  }
}
